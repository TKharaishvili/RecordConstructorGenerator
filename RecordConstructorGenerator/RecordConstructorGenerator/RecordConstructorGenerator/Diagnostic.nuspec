<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>RecordConstructorGenerator</id>
    <version>2.0.0.0</version>
    <title>RecordConstructorGenerator</title>
    <authors>Nobuyuki Iwanaga</authors>
    <owners>Nobuyuki Iwanaga</owners>
    <licenseUrl>https://github.com/ufcpp/RecordConstructorGenerator/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/ufcpp/RecordConstructorGenerator/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Code Fix for a record (immutable class/struct) constructor generated from get-only properties</description>
    <releaseNotes>Generate the constructor in a separate partial file.</releaseNotes>
    <copyright>Nobuyuki Iwanaga</copyright>
    <tags>record type, immutable type, code generator, analyzers</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="*.dll" target="tools\analyzers\" exclude="**\Microsoft.CodeAnalysis.*;**\System.Collections.Immutable.*;**\System.Reflection.Metadata.*" />
    <file src="tools\*.ps1" target="tools\" />
  </files>
</package>